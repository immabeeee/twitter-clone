{
  "version": 1,
  "projects": {
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api"],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-interfaces": {
      "root": "libs/api-interfaces",
      "sourceRoot": "libs/api-interfaces/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/api-interfaces/**/*.ts"]
          }
        }
      }
    },
    "directives-auto-resize-textarea": {
      "projectType": "library",
      "root": "libs/directives/auto-resize-textarea",
      "sourceRoot": "libs/directives/auto-resize-textarea/src",
      "prefix": "twitter-clone-ws",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/directives/auto-resize-textarea"],
          "options": {
            "jestConfig": "libs/directives/auto-resize-textarea/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/directives/auto-resize-textarea/src/**/*.ts",
              "libs/directives/auto-resize-textarea/src/**/*.html"
            ]
          }
        }
      }
    },
    "shared-assets": {
      "projectType": "library",
      "root": "libs/shared/assets",
      "sourceRoot": "libs/shared/assets/src",
      "prefix": "twitter-clone-ws",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/assets"],
          "options": {
            "jestConfig": "libs/shared/assets/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/assets/src/**/*.ts",
              "libs/shared/assets/src/**/*.html"
            ]
          }
        }
      }
    },
    "shared-theme": {
      "projectType": "library",
      "root": "libs/shared/theme",
      "sourceRoot": "libs/shared/theme/src",
      "prefix": "twitter-clone-ws",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/theme"],
          "options": {
            "jestConfig": "libs/shared/theme/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/theme/src/**/*.ts",
              "libs/shared/theme/src/**/*.html"
            ]
          }
        }
      }
    },
    "shared-variables": {
      "projectType": "library",
      "root": "libs/shared/variables",
      "sourceRoot": "libs/shared/variables/src",
      "prefix": "twitter-clone-ws",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/variables"],
          "options": {
            "jestConfig": "libs/shared/variables/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/variables/src/**/*.ts",
              "libs/shared/variables/src/**/*.html"
            ]
          }
        }
      }
    },
    "twitter-clone-web": {
      "projectType": "application",
      "root": "apps/twitter-clone-web",
      "sourceRoot": "apps/twitter-clone-web/src",
      "prefix": "twitter-clone-ws",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/twitter-clone-web",
            "index": "apps/twitter-clone-web/src/index.html",
            "main": "apps/twitter-clone-web/src/main.ts",
            "polyfills": "apps/twitter-clone-web/src/polyfills.ts",
            "tsConfig": "apps/twitter-clone-web/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/twitter-clone-web/src/favicon.ico",
              "apps/twitter-clone-web/src/assets",
              {
                "glob": "**/*",
                "input": "./libs/shared/assets/src/lib",
                "output": "./assets"
              }
            ],
            "styles": [
              "apps/twitter-clone-web/src/styles.scss",
              "./libs/shared/theme/src/lib/theme-twitter.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/twitter-clone-web/src/environments/environment.ts",
                  "with": "apps/twitter-clone-web/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "twitter-clone-web:build:production"
            },
            "development": {
              "browserTarget": "twitter-clone-web:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "apps/twitter-clone-web/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "twitter-clone-web:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/twitter-clone-web/src/**/*.ts",
              "apps/twitter-clone-web/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/twitter-clone-web"],
          "options": {
            "jestConfig": "apps/twitter-clone-web/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "twitter-clone-web-e2e": {
      "root": "apps/twitter-clone-web-e2e",
      "sourceRoot": "apps/twitter-clone-web-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/twitter-clone-web-e2e/cypress.json",
            "tsConfig": "apps/twitter-clone-web-e2e/tsconfig.e2e.json",
            "devServerTarget": "twitter-clone-web:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "twitter-clone-web:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/twitter-clone-web-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "ui-button": {
      "projectType": "library",
      "root": "libs/ui-button",
      "sourceRoot": "libs/ui-button/src",
      "prefix": "twitter-clone-ws",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui-button"],
          "options": {
            "jestConfig": "libs/ui-button/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui-button/src/**/*.ts",
              "libs/ui-button/src/**/*.html"
            ]
          }
        }
      }
    },
    "ui-sidebar-link": {
      "projectType": "library",
      "root": "libs/ui-sidebar-link",
      "sourceRoot": "libs/ui-sidebar-link/src",
      "prefix": "twitter-clone-ws",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui-sidebar-link"],
          "options": {
            "jestConfig": "libs/ui-sidebar-link/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui-sidebar-link/src/**/*.ts",
              "libs/ui-sidebar-link/src/**/*.html"
            ]
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "libs/ui-sidebar-link/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/ui-sidebar-link",
            "config": {
              "configFolder": "libs/ui-sidebar-link/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "ui-user-avatar": {
      "projectType": "library",
      "root": "libs/ui-user-avatar",
      "sourceRoot": "libs/ui-user-avatar/src",
      "prefix": "twitter-clone-ws",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui-user-avatar"],
          "options": {
            "jestConfig": "libs/ui-user-avatar/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui-user-avatar/src/**/*.ts",
              "libs/ui-user-avatar/src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "twitter-clone-web"
}
